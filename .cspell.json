{
  "language": "en,en-US",
  "version": "0.2",
  "ignorePaths": [
    "**/.cspell.json",
    "**/.gitignore",
    "**/.git/**",
    "**/node_modules/**",
    "**/package-lock.json",
    ".github/workflows/*.yml"
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": ["/`+([\\s\\S]*?)`+/g", "/^```[\\s\\S]*?^```/gm"]
    }
  ],
  "words": ["CCWM", "DPR", "glama", "offseason", "OPR", "withinfocus"]
}
